{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug The Things Stack",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/ttn-lw-stack",
            "args": ["-c", "${workspaceFolder}/config/stack/ttn-lw-stack.yml", "start"],
            "env": {},
            "cwd": "${workspaceFolder}"
            // "envFile": "${workspaceFolder}/.env"
        }
    ]
}
    